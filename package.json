{
  "name": "spotify",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run build:obfuscate",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky",
    "postbuild": "next-sitemap",
    "build:obfuscate": "next-css-obfuscator",
    "build:stats": "ANALYZE=true npm run build",
    "cmd:start:prod": "npm run prisma:migrate:prod && node server.js",
    "docker:dev:build": "docker compose -f docker-compose.dev.yml build app-dev",
    "docker:dev:up": "docker compose -f docker-compose.dev.yml up -d",
    "docker:dev:down": "docker compose -f docker-compose.dev.yml down --remove-orphans",
    "docker:stage:build": "npm run docker:stage:up db-stage && dotenv -e .env.staging.local -- docker compose -f docker-compose.stage.yml build app-stage",
    "docker:stage:up": "docker compose -f docker-compose.stage.yml up -d",
    "docker:stage:down": "docker compose -f docker-compose.stage.yml down --remove-orphans",
    "docker:prod:build": "dotenv -e envs/production/.env.production.docker.local -- npm run docker:prod:up db-prod && dotenv -e .env.staging.local -- docker compose -f docker-compose.prod.yml build app-prod",
    "prisma:migrate:dev:env": "dotenv -e .env.development.local -- npx prisma migrate dev --name",
    "prisma:push:dev:env": "dotenv -e .env.development.local -- npx prisma db push",
    "prisma:studio:dev:env": "dotenv -e .env.development.local -- npx prisma studio",
    "prisma:seed:dev:env": "dotenv -e .env.development.local -- npx prisma db seed",
    "prisma:reset:dev:env": "dotenv -e .env.development.local -- npx prisma migrate reset dev",
    "prisma:migrate:stage": "npx prisma migrate deploy",
    "prisma:migrate:stage:env": "dotenv -e .env.staging.local -- npm run prisma:migrate:prod",
    "email:dev:env": "dotenv -e envs/development/.env.development.docker -e envs/development/.env.development.docker.local -- email dev --dir src/email --port 4000"
  },
  "dependencies": {
    "@auth/core": "^0.30.0",
    "@auth/prisma-adapter": "^2.0.0",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/client": "^5.13.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@react-email/components": "^0.0.16",
    "@react-email/tailwind": "^0.0.15",
    "@stripe/react-stripe-js": "^2.7.0",
    "@stripe/stripe-js": "^3.3.0",
    "@t3-oss/env-nextjs": "^0.9.2",
    "@tanstack/react-query": "^5.32.1",
    "@tanstack/react-query-devtools": "^5.32.1",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv-cli": "^7.4.1",
    "input-otp": "^1.2.4",
    "ky": "^1.2.3",
    "next": "14.2.1",
    "next-auth": "^5.0.0-beta.16",
    "prisma": "^5.13.0",
    "react": "^18",
    "react-dom": "^18",
    "react-email": "^2.1.1",
    "react-hook-form": "^7.51.3",
    "react-icons": "^5.1.0",
    "react-intersection-observer": "^9.10.2",
    "react-spinners": "^0.13.8",
    "resend": "^3.2.0",
    "server-only": "^0.0.1",
    "sharp": "^0.33.3",
    "stripe": "^15.2.0",
    "tailwind-merge": "^2.2.2",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss-animate": "^1.0.7",
    "use-sound": "^4.0.1",
    "usehooks-ts": "^3.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@hookform/devtools": "^4.3.1",
    "@next/bundle-analyzer": "^14.2.1",
    "@tanstack/eslint-plugin-query": "^5.28.11",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.2.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-unused-imports": "^3.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "next-css-obfuscator": "^2.2.15",
    "next-sitemap": "^4.2.3",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsc-files": "^1.1.4",
    "typescript": "^5"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
